@model UniCampaignE.Core.Models.Tenant
@{
    Layout = "~/Views/Shared/_UniCampaignAdminLayout.cshtml";
}
@section page_content{
    <div class="content-heading">
        @UniCampaignE.Web.Localization.Configuration.CONFIGURATION_TITLE
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-flat panel-primary">
                <div class="panel-heading">
                    <div class="panel-title">
                      @*@Model.Name*@ Configuration
                    </div>
                </div>
                <div class="panel-body">
                    @using (Ajax.BeginForm(nameof(UniCampaignE.Web.Areas.Admin.Controllers.TenantController.Save), new AjaxOptions() { HttpMethod = "POST", LoadingElementId = "loader", OnSuccess = "globalAjaxSuccessHandler", OnFailure = "globalAjaxErrorHandler" }))
                    {
                        <div class="col-lg-12">
                            @Html.EditorForModel("TenantConfiguration")
                            <div class="form-group mt-lg">
                                <input type="submit" value="@UniCampaignE.Web.Localization.Comman.SAVE_BUTTON_TEXT" class="btn btn-primary pull-right" />
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
}
@section page_scripts{
}
;